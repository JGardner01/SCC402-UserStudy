<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scc402.userstudy.StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="SCC 402 User Study">
         <font>
            <Font name="Calibri Light" size="64.0" />
         </font>
      </Text>
      <Text fx:id="introText" strokeType="OUTSIDE" strokeWidth="0.0" text="Part _: _ User Interface">
         <font>
            <Font name="Calibri" size="32.0" />
         </font>
      </Text>
      <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClick" text="Begin _ UI Demo">
         <font>
            <Font name="Calibri" size="18.0" />
         </font></Button>
   </children>
</VBox>
